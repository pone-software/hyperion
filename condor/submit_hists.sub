+SingularityImage = "/data/p-one/chaack/container/pytorch-geo.sif"
executable = /bin/env
arguments = python /data/p-one/chaack/hyperion/generate_tres_histograms.py -i $(infile) -o $(outfile) -r 0.21 -m /data/p-one/chaack/hyperion/hyperion/resources/medium.json -s $(seed) --n-thetas 150 --tts $(tts)
Requirements = HasSingularity
request_gpus = 0
log = logs/log
output = logs/out_fit.$(CLUSTER)
error = logs/err_fit.$(CLUSTER)

when_to_transfer_output = on_exit
should_transfer_files = yes
transfer_input_files = $(infile)
queue 1
